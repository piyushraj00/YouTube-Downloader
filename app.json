{
  "name": "YouTube Downloader Bot"
  "description": "A simple YouTube downloader Bot",
  "keywords": [
    "telegram",
    "youtube",
    "downloader"
  ],
  "success_url": "https://github.com/piyush00",
  "website": "https://fayas.cf",
  "repository": "https://github.com/piyush00/YouTube-Downloader",
  "env": {
    "WEBHOOK": {
      "description": "Leave this as it is!",
      "value": "ANYTHING"
    },
    "BOT_TOKEN": {
      "description": "Your bot token from @botfather"
    },
    "APP_ID": {
      "description": "Get this value from https://my.telegram.org"
    },
    "API_HASH": {
      "description": "Get this value from https://my.telegram.org"
    },
    "AUTH_USERS": {
      "description": "allow only pre-defined users to use this bot"
    },
    "TIME_LIMIT": {
      "description": "Set timeout for subprocess (In seconds)",
      "value": "0"
    }
  },
  "addons": [
    {
      "plan": "heroku-postgresql",
        "options": {
          "version": "12"
        }
      }
  ],
  "buildpacks": [{
    "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest"
  }, {
    "url": "https://github.com/opendoor-labs/heroku-buildpack-p7zip"
  }, {
    "url": "heroku/python"
  }],
  "formation": {
    "worker": {
      "quantity": 1,
      "size": "free"
    }
  }
}
